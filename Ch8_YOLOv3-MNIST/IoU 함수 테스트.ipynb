    "import numpy as np\n",
    "from bbox_iou import bbox_iou\n",
    "\n",
    "# box format: (xmin, ymin, xmax, ymax)\n",
    "box1 = np.array([0, 0, 2, 2])\n",
    "box2 = np.array([1, 1, 3, 3])\n",
    "\n",
    "iou = bbox_iou(box1, box2, method=\"iou\", isTrain=False)\n",
    "print(\"IoU (partial overlap):\", iou)"
   ]
